   "source": [
    "ggplot(ans1, aes(x = x, y = y)) +\n",
    "  geom_point() +\n",
    "  geom_abline(intercept= lmodel.ans1$coefficients[1], slope = lmodel.ans1$coefficients[2], col = \"red\")"
